{
  "name": "unplugin-config",
  "type": "module",
  "version": "0.1.4",
  "packageManager": "pnpm@8.15.5",
  "description": "Register global imports on demand for Vite and Webpack",
  "author": "Kirk Lin <https://github.com/kirklin>",
  "license": "MIT",
  "funding": "https://www.buymeacoffee.com/linkirk",
  "homepage": "https://github.com/kirklin/unplugin-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirklin/unplugin-config.git"
  },
  "bugs": {
    "url": "https://github.com/kirklin/unplugin-config/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@kirklin/logger": "0.0.2",
    "html-entities": "^2.5.2",
    "jsdom": "^24.0.0",
    "unplugin": "^1.10.0"
  },
  "devDependencies": {
    "@kirklin/eslint-config": "^2.1.0",
    "@nuxt/kit": "^3.11.1",
    "@nuxt/schema": "^3.11.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "bumpp": "^9.4.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.4.0",
    "webpack": "^5.90.3"
  }
}
